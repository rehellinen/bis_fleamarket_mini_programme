.order-container{
  background-color: white;
  width: 750rpx;
}

.single-container{
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #e9e9e9
}

.single-container view{
  display: flex;
  flex-direction: column;
  padding: 15rpx 25rpx;
}

.status{
  font-size: 34rpx;
}

.order-name{
  font-size: 30rpx;
  margin-top: 10rpx;
}

.price{
  font-size: 30rpx;
}

.time{
  font-size: 28rpx;
  margin-top: 15rpx;
}